{
  "name": "material-react-js",
  "version": "5.0.0-prerelease.3",
  "description": "Meterial design components for React",
  "keywords": [
    "material components",
    "material design",
    "react"
  ],
  "scripts": {
    "docs": "rimraf ./docs/api && jsdoc -R README.md -c ./jsdoc.json -r",
    "lint": "eslint --ext .js,.jsx .",
    "open:cover": "npm run test:cover && open-cli ./coverage/lcov-report/index.html",
    "open:docs": "npm run docs && open-cli ./docs/api/index.html",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:cover": "npm test -- --coverage"
  },
  "repository": {
